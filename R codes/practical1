data1 = data.frame(
  Habitat_A = c(22.4, 25.1, 21.9, 23.5, 24.3, 22.7, 25.0, 23.9, 24.7, 23.2),
  Habitat_B= c(23.0, 24.3, 25.5, 23.9, 26.2, 25.0, 24.5, 26.1, 25.8, 24.7),
  Habitat_C = c(19.5, 18.8, 20.1, 19.7, 18.3, 19.9, 20.2, 18.6, 19.3, 20.0)
)
sapply(data1, function(x) c(Mean = mean(x), 
                            Median = median(x), 
                            Variance = var(x),
                            Range =max(x)-min(x),
                            Variance= var(x)))
